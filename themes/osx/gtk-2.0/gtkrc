	GtkScrollbar	::slider_width		= 16
	GtkScrollbar	::stepper_size		= 16

	engine "pixmap" {
		image {
			function	= BOX
			orientation	= VERTICAL
			file		= "assets/scroll-background.png"
		}
	}
}
