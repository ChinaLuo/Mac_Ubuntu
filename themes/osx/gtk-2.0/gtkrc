			orientation	= HORIZONTAL
			file		= "assets/null.png"
			border		= {0, 0, 0, 0}
			stretch		= TRUE
    		}

  		image {
			function	= HANDLE
			overlay_file	= "assets/handle.png"
			overlay_border	= {0, 0, 0, 0}
			overlay_stretch	= FALSE
			orientation	= VERTICAL
			file		= "assets/null.png"
			border		= {0, 0, 0, 0}
			stretch		= TRUE
		}
	}
}
